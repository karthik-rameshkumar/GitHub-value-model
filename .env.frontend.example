# Frontend Environment Variables Template
# Copy this file to .env in the frontend directory and fill in the values

# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_TIMEOUT=10000

# Application Configuration
REACT_APP_NAME=ESSP Dashboard
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true

# Polling Configuration
REACT_APP_POLLING_INTERVAL=30000

# Chart Configuration
REACT_APP_CHART_THEME=light
REACT_APP_DEFAULT_DATE_RANGE=30

# GitHub Integration
REACT_APP_GITHUB_CLIENT_ID=your-github-oauth-client-id

# Development
GENERATE_SOURCEMAP=true
FAST_REFRESH=true
CHOKIDAR_USEPOLLING=true